{
    "env": {
        "commonjs": true,
        "es6": true,
        "node": true
    },
    "plugins": ["prettier"],
    "extends": ["prettier", "eslint:recommended"],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly",
        "document": true
    },
    "parserOptions": {
        "ecmaVersion": 2018
    },
    "rules": {
      "prettier/prettier": "error"
    }
}